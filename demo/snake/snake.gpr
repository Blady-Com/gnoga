with "../../src/gnoga.gpr";

project AdaBlog is

   for Languages use ("Ada");
   for Source_Dirs use (".");
   for Object_Dir use "../../obj";
   for Exec_Dir use "../../bin";
   for Main use ("snake-main.adb");

   package Builder is
      for Executable ("snake-main.adb") use "snake";
   end Builder;
      
   package Compiler is
      for Default_Switches ("ada") use ("-gnat05", "-g");
   end Compiler;
end AdaBlog;
