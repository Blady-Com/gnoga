with "../../src/gnoga.gpr";

project AdaBlog is

   for Languages use ("Ada");
   for Source_Dirs use (".");
   for Object_Dir use "../../obj";
   for Exec_Dir use "../../bin";
   for Main use ("adablog-main.adb");

   package Builder is
      for Executable ("adablog-main.adb") use "adablog";
   end Builder;

   package Binder is
     for Default_Switches ("ada") use ("-E");
   end Binder;
end AdaBlog;
