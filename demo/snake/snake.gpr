with "../../src/gnoga.gpr";

project Snake is

   for Languages use ("Ada");
   for Source_Dirs use (".");
   for Object_Dir use "../../obj";
   for Exec_Dir use "../../bin";
   for Main use ("snake-main.adb");

   package Builder is
      for Executable ("snake-main.adb") use "snake";
   end Builder;
      
   package Binder is
     for Default_Switches ("ada") use ("-E");
   end Binder;
end Snake;
