with "../../src/gnoga.gpr";

project AdaEdit is

   for Languages use ("Ada");
   for Source_Dirs use (".");
   for Object_Dir use "../../obj";
   for Exec_Dir use "../../bin";
   for Main use ("adaedit.adb");

   package Builder is
      for Executable ("adaedit.adb") use "adaedit";
   end Builder;
      
   package Binder is
     for Default_Switches ("ada") use ("-E");
   end Binder;
end AdaEdit;
